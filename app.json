{
  "expo": {
    "name": "AquaVita",
    "slug": "aquavita",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "assets/images/splash-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "plugins": [
      "expo-camera"
    ]
  }
}